        "stopOnEntry": true,
        "cwd": "${fileDirname}",
        "port": 9000,
        "runtimeExecutable": "D:\\Programmes\\wamp64\\bin\\php\\php7.3.5\\php.exe"
    },
        {
            "type": "firefox",
            "request": "attach",
            "name": "Attach"
        },
        
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Launch add-on",
            "addonPath": "${workspaceFolder}"
        }
    ]
